[[source]]
name = "pypi"
url = "https://pypi.org/simple"
verify_ssl = true

[dev-packages]
pylint = "*"
twine = "*"

[packages]
click = "*"
plette = "==0.4.4"

[requires]
python_version = "3.8"

[scripts]
package = "pipenv run python setup.py sdist --formats=gztar"
pubtest = "pipenv run twine upload --repository pypitest dist/*"
publish = "pipenv run twine upload --repository pypi dist/*"
